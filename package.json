  "author": "Andreas Jantschnig <andreas.jantschnig@tv-insight.com>",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"yarn serve-scripts\" \"yarn serve-test -c ../serve.json\"",
    "build": "rm -rf ./dist && gulp",
    "serve-scripts": "ts-node app.ts",
    "serve-test": "serve test/ -p 8080",
    "serve-dist": "serve dist/ -p 4040 -c ../serve.json",
    "test": "jest --verbose",
    "ci": "start-server-and-test \"concurrently 'yarn serve-scripts' 'yarn serve-test' --hide 0,1\" http://localhost:8080 test",
    "lint": "eslint src/**"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/gulp": "^4.0.17",
    "@types/gulp-htmlmin": "^1.3.36",
    "@types/gulp-if": "^3.0.4",
    "@types/gulp-rename": "^2.0.6",
    "@types/gulp-size": "^4.0.3",
    "@types/gulp-terser": "^1.2.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "concurrently": "^8.2.2",
    "ejs": "^3.1.10",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-prettier": "^5.2.1",
    "express": "^4.19.2",
    "gulp": "^5.0.0",
    "gulp-ejs": "^5.1.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-minify-inline": "^1.1.0",
    "gulp-rename": "^2.0.0",
    "gulp-size": "^5.0.0",
    "gulp-terser": "^2.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "puppeteer": "^22.15.0",
    "serve": "^14.2.3",
    "start-server-and-test": "^2.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
